<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.github.cookiesjuice.entity.Setu" table="cookies_bot_setu">
        <id name="id" type="java.lang.Long" column="setu_id">
            <generator class="native"/>
        </id>

        <property name="name" type="java.lang.String" column="setu_name"/>

        <property name="author" type="java.util.Date" column="setu_author"/>

        <set name="gradeUsers" inverse="true">
            <key column="setu_id"/>
            <many-to-many class="com.github.cookiesjuice.entity.User" column="user_id"/>
        </set>

        <set name="favoriteUsers" inverse="true">
            <key column="setu_id"/>
            <many-to-many class="com.github.cookiesjuice.entity.User" column="user_id"/>
        </set>

        <many-to-one name="uploadUser" class="com.github.cookiesjuice.entity.User" column="setu_id"/>

        <property name="uploadTime" type="java.util.Date" column="setu_upload_time"/>
    </class>
</hibernate-mapping>