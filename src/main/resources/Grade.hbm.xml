<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.github.cookiesjuice.cookiesbot.entity.Grade" table="cookies_bot_grade">
        <id name="id" type="java.lang.Long" column="tag_id">
            <generator class="native"/>
        </id>

        <property name="number" type="java.lang.Integer" column="grade_number" not-null="true"/>

        <many-to-one name="user" class="com.github.cookiesjuice.entity.User" column="user_id"/>

        <many-to-one name="setu" class="com.github.cookiesjuice.entity.Setu" column="setu_id"/>
    </class>
</hibernate-mapping>